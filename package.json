{
  "name": "vh-packages",
  "version": "0.0.1",
  "description": "Vanhack Monorepo",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/durandneto/monorep.git"
  },
  "license": "MIT",
  "useWorkspaces": true,
  "author": "Durand Neto",
  "scripts": {
    "start": "NODE_ENV=development start-storybook -p 9001 -c .storybook",
    "test": "jest --coverage --verbose",
    "bootstrap": "lerna bootstrap",
    "build": "lerna exec --parallel -- babel  --presets @babel/preset-react --plugins=@babel/plugin-syntax-dynamic-import,@babel/plugin-proposal-class-properties  src -d dist --ignore spec.js,story.js,node_modules/**/*",
    "build-storybook": "build-storybook",
    "publish": "lerna publish --skip-git --skip-npm",
    "prepublish": "npm run build",
    "precommit": "npm test",
    "postinstall": "npm run bootstrap",
    "storybook": "start-storybook -p 6006"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@storybook/addon-actions": "^5.0.1",
    "@storybook/addon-console": "^1.1.0",
    "@storybook/addon-info": "^4.1.12",
    "@storybook/addon-links": "^5.0.1",
    "@storybook/addons": "^5.0.1",
    "@storybook/react": "^5.0.1",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "glob-loader": "^0.3.0",
    "jest": "^24.5.0",
    "lerna": "^2.0.0-rc.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "styled-components": "^4.1.3"
  },
  "packages": [
    "packages/*"
  ],
  "dependencies": {
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "prop-types": "^15.7.2"
  }
}
